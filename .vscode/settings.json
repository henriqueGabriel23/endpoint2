  "typescript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "redhat.telemetry.enabled": true,
  "conventionalCommits.lineBreak": "\\n",
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "vs-kubernetes": {
    "vscode-kubernetes.kubectl-path.linux": "/home/user/.local/state/vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/user/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.helm-path.linux": "/home/user/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open"
